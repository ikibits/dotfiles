{
  "extends": [
    "eslint:recommended",
    "plugin:jsx-a11y/recommended",
    "plugin:react/recommended"
  ],
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "mocha": true,
    "mongo": true,
    "node": true
  },
  "rules": {
    "react/jsx-closing-bracket-location": "error",
    "react/jsx-no-bind": "error",
    "react/no-multi-comp": "error",
    "react/prop-types": "warn"
  }
}
