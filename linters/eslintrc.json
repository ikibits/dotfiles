{
  "env": {
    "browser": true,
    "mocha": true,
    "mongo": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:jsx-a11y/recommended",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "jsx-a11y",
    "react"
  ],
  "rules": {
    "no-console": "warn",
    "react/jsx-closing-bracket-location": "error",
    "react/jsx-no-bind": "error",
    "react/no-multi-comp": "error"
  }
}
